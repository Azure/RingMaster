<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="ThreadCount" value="64" />
    <add key="MinDataSize" value="16" />
    <add key="MaxDataSize" value="256" />
    <add key="BatchOpCount" value="32" />
    <add key="TestCaseSeconds" value="60" />
    <add key="RequestTimeout" value="100000" />

    <!-- parameters for bulk watcher test -->
    <add key="BulkWatcherThreadCount" value="-1" />
    <add key="BulkWatcherMinDataSize" value="32" />
    <add key="BulkWatcherMaxDataSize" value="128" />
    <add key="PartitionCount" value="1024" />
    <add key="NodeCountPerPartition" value="16" />

    <!-- Number of RingMasterClient, i.e. the connection to the backend -->
    <add key="ChannelCount" value="10" />
    <add key="TestRepetitions" value="50" />
    <add key="BulkWatcherRequestTimeout" value="100000" />

    <!-- Number of async tasks to await in a batch -->
    <add key="AsyncTaskCount" value="64" />

    <!-- Mdm settings -->
    <add key="MdmEnabled" value="false"/>
    <add key="MdmAccountName" value="SDNPubSub"/>
    <add key="Environment" value="prod" />
    <add key="Tenant" value="vega-ussc" />
  </appSettings>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>

  <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <add name="configConsoleListener"
             type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
