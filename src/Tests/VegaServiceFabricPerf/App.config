<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="ThreadCount" value="64" />
    <add key="MinDataSize" value="32" />
    <add key="MaxDataSize" value="128" />
    <add key="BatchOpCount" value="32" />
    <add key="TestCaseSeconds" value="200" />
    <add key="RequestTimeout" value="100000" />
    <!-- parameters for bulk watcher test -->
    <add key="BulkWatcherThreadCount" value="32" />
    <add key="BulkWatcherMinDataSize" value="256" />
    <add key="BulkWatcherMaxDataSize" value="16384" />
    <add key="PartitionCount" value="10240" />
    <add key="NodeCountPerPartition" value="16" />
    <add key="ChannelCount" value="10" />
    <add key="TestRepetitions" value="100" />
    <add key="BulkWatcherRequestTimeout" value="100000" />
  </appSettings>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>

  <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <add name="configConsoleListener"
             type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>