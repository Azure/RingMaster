{
  "DebuggerAttachTimeout": "0",
  "InMemoryPersistence": "false",
  "SSL.UseSSL": "false",
  "RingMaster.SecondaryPreprocessor.RunOnMultiplesOf": "0",
  "RingMaster.DebugWatcherLocations": "false",
  "RingMaster.DontStartPseudoNodes": "false",

  "WireBackup.Enable": "false",
  "WireBackup.Path": ".\\WireBackups",
  "WireBackup.MaxEvents": "10000",
  "WireBackup.MaxTimeInSeconds": "60",
  "WireBackup.MaxEventsBetweenSnapshots": "100000",
  "WireBackup.MaxTimeBetweenSnapshotsInSeconds": "600",
  "WireBackup.RetentionSeconds": "7776000",

  "RingMaster.Transport.ClientConnection.MaxUnFlushedMillis": "2",
  "RingMaster.Transport.ClientConnection.MaxUnFlushedResponses": "500",
  "RingMaster.Transport.ClientConnection.MaxThreadsPerConnection": "2",
  "RingMaster.Transport.ClientConnection.BufferSizeInBytes": "40960",

  "RingMaster.Transport.ClientSession.MaxUnFlushedMillis": "2",
  "RingMaster.Transport.ClientSession.MaxUnFlushedResponses": "1000",
  "RingMaster.Transport.ClientSession.MaxThreadsPerClient": "2",
  "RingMaster.Transport.ClientSession.BufferSizeInBytes": "40960",

  "RingMaster.Paths.NotAllowedToFail": "/",
  "RingMaster.Paths.IgnoreForLockDown": "/$tm/lastAppliedTransactionId, /$tm/transactionCrc",

  "RingMaster.LockStrategy": "MultiLevelRWLocks",

  "WinFabPersistence.HotSecondary": "false",
  "WinFabPersistence.EnableSFNotificationHandler": "false",
  "WinFabPersistence.FixStatDuringLoad": "false",

  "LogFolder": "c:\\Resources\\Directory"
}
