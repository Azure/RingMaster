<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(EnvironmentConfig)" />
  <Import Project="$(PkgNuproj)\tools\NuProj.props" Condition="'$(OSSBUILD)' != ''" />
  <Import Project="..\NuprojCommon.props" />
  <PropertyGroup Label="Globals">
    <ProjectGuid>c6ede946-e5fb-48cc-ba85-c181826e9a18</ProjectGuid>
  </PropertyGroup>
  <PropertyGroup>
    <BinariesBuildTypeArchDirectory Condition="'$(BinariesBuildTypeArchDirectory)' == ''">$(OUTPUTROOT)\$(BuildType)-$(BuildArchitecture)</BinariesBuildTypeArchDirectory>
    <Id>VegaServiceFabricApplication$(NamePostfix)</Id>
    <Title>Vega Service Fabric Application</Title>
    <Authors>vegab@microsoft.com</Authors>
    <Owners>vegab@microsoft.com</Owners>
    <Description>Vega Persistence layer that uses ReliableDictionary to persist data</Description>
    <Copyright>Copyright (c) Microsoft Corporation</Copyright>
    <Tags>Vega</Tags>
    <GenerateSymbolPackage>false</GenerateSymbolPackage>
    <IncludeBuiltProjectOutputGroupDependencies>false</IncludeBuiltProjectOutputGroupDependencies>
    <UserName Condition="'$(Q_SYNCTO_CHANGELIST)' != ''">cloudbuilduser</UserName>
  </PropertyGroup>
  <ItemGroup>
    <ProjectRef Include="$(SRCROOT)\Common\SecureTransport\src\SecureTransport.csproj" />
    <ProjectRef Include="$(SRCROOT)\Applications\RingMasterApplication\RingMasterService\RingMasterService.csproj" />
    <ProjectRef Include="$(SRCROOT)\Applications\RingMasterApplication\RingMasterWatchdog\RingMasterWatchdog.csproj" />
    <ProjectRef Include="$(SRCROOT)\Applications\RingMasterApplication\AppPublish\AppPublish.proj" />
    <ProjectReference Include="@(ProjectRef)">
      <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <File Include="$(BinariesBuildTypeArchDirectory)\RingMasterApplication.sfpkg" />
    <QCustomInput Include="$(BinariesBuildTypeArchDirectory)\RingMasterApplication.sfpkg" />
  </ItemGroup>
  <ItemGroup>
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.RingMasterService\Microsoft.RingMaster.RingMasterService.exe" />
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.RingMasterService\Microsoft.RingMaster.RingMasterService.pdb" />
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.RingMasterWatchdog\Microsoft.RingMaster.RingMasterWatchdog.exe" />
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.RingMasterWatchdog\Microsoft.RingMaster.RingMasterWatchdog.pdb" />
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.SecureTransport\Microsoft.RingMaster.SecureTransport.dll" />
    <libs Include="$(BinariesBuildTypeArchDirectory)\Microsoft.RingMaster.SecureTransport\Microsoft.RingMaster.SecureTransport.pdb" />
    <Library Include="@(libs)">
      <TargetFramework>net45</TargetFramework>
    </Library>
  </ItemGroup>
  <Import Project="$(PkgNuProj)\tools\NuProj.targets" Condition="'$(OSSBUILD)' != ''" />
  <Target Name="AfterBuild" Condition="'$(OSSBUILD)' != ''">
    <ItemGroup>
      <nupkgs Include="$(IntermediateOutputPath)\*.nupkg" />
    </ItemGroup>
    <Copy SourceFiles="@(nupkgs)" DestinationFolder="$(OUTPUTROOT)\packages\$(BuildType)" />
  </Target>
  <Import Project="$(PkgNuProj)\NuProj.Targets" Condition="'$(OSSBUILD)' == ''" />
</Project>
